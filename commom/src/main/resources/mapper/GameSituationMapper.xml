<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.trevor.dao.GameSituationMapper">

    <insert id="insertOne">
        INSERT INTO
        `game_situation` (room_record_id ,game_num ,game_situation)
        VALUES
        (#{gameSituation.roomRecordId},#{gameSituation.gameNum},#{gameSituation.gameSituation})
    </insert>
</mapper>